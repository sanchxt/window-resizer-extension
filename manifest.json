{
  "manifest_version": 3,
  "name": "Window Resizer",
  "description": "Quickly check a website's responsiveness at different breakpoints",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "permissions": ["windows"],
}
